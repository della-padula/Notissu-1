<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.openSource.OpenSourceActivity">

    <com.yourssu.notissu.customView.NotissuTopbar
        android:id="@+id/openSourceTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:setBackButton="true">

    </com.yourssu.notissu.customView.NotissuTopbar>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/openSourceRecycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/top_bar_height"
        tools:listitem="@layout/item_open_source"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/openSourceTitle" />

</androidx.constraintlayout.widget.ConstraintLayout>